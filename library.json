{
    "name": "kic_notation",
    "version": "1.0.0",
    "description": "High-performance KIC notation parser optimized for ESP32. Features O(1) direct mapping, O(M) stride search, and zero-allocation parsing.",
    "keywords": "kic, parser, esp32, embedded, real-time",
    "repository": {
        "type": "git",
        "url": "https://github.com/Tomarun028831/kic_notation.git"
    },
    "authors": [
        {
            "name": "Tomarun028831",
            "maintainer": true
        }
    ],
    "license": "MIT",
    "frameworks": "*",
    "platforms": "espressif32",
    "build": {
        "flags": [
            "-I modules/kic_parser",
            "-I modules/kic_timestamp"
        ],
        "srcFilter": [
            "+<modules/kic_parser/*.c>",
            "+<modules/kic_timestamp/*.c>"
        ]
    }
}
